<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:background="@drawable/reddish_button_background">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/list"
            android:name="customer.homepage.clientinfo.carinfo.cardetail.CarSpecificServiceFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            app:layoutManager="LinearLayoutManager"
            tools:context="customer.homepage.clientinfo.carinfo.cardetail.CarSpecificServiceFragment"
            tools:listitem="@layout/fragment_car_specific_service_item" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>